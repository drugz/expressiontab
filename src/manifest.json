{
     "name": "Expression Tab",
     "version": "1.0",
     "description": "Expression Tab",
     "host_permissions": [
         "http://*/",
         "https://*/"
       ],
     "permissions": [
          "bookmarks",
          "history"
        ],
     "chrome_url_overrides": {
         "newtab": "newtab/index.html"
        },
        "action": {
             "default_icon": {
             "16": "assets/icon16.png",
             "32": "assets/icon32.png",
             "48": "assets/icon48.png",
             "128": "assets/icon128.png"
             }
        },
        "icons": {
             "16": "assets/icon16.png",
             "32": "assets/icon32.png",
             "48": "assets/icon48.png",
             "128": "assets/icon128.png"
        },
        "content_scripts": [
          {
              "matches": ["<all_urls>"]
          }
      ],
     "manifest_version": 3
 }